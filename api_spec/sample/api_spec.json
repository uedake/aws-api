{
    "name": "sample-api",
    "stage": {
        "$default": {
            "branch": "main",
            "bucket": "{$account}-{$api}"
        },
        "dev": {
            "branch": "dev",
            "bucket": "{$account}-{$api}"
        },
        "test": {
            "branch": "test",
            "bucket": "{$account}-{$api}-test"
        }
    },
    "s3": {
        "{$account}-{$api}": {
            "public_read": true,
            "website_hosting": true
        },
        "{$account}-{$api}-test": {
            "public_read": false
        }
    },
    "apigw": {
        "description": "api for calling sample lambdas",
        "route": {
            "pipeline_entry": [
                "ANY /pipeline_entry",
                "ANY /pipeline_entry/{z}"
            ],
            "s3_access": [
                "ANY /s3_access"
            ],
            "echo": [
                "ANY /echo/{path1}/{path2}",
                "ANY /echo/{path1}",
                "ANY /echo"
            ],
            "numpy": [
                "ANY /numpy",
                "ANY /numpy/{z}"
            ]
        }
    },
    "sqs_for_lambda": {
        "pipeline_process": {
            "additional_timeout": 10
        },
        "echo": {
            "additional_timeout": 10
        }
    },
    "lambda_func": {
        "pipeline_entry": {
            "code": "src_lambda/pipeline_entry/src",
            "test": "src_lambda/pipeline_entry/test/test.json",
            "timeout": 3,
            "queue_next": "pipeline_process"
        },
        "pipeline_process": {
            "code": "src_lambda/pipeline_process/src",
            "timeout": 10
        },
        "s3_access": {
            "code": "src_lambda/s3_access/src",
            "test": "src_lambda/s3_access/test/test.json",
            "timeout": 10
        },
        "echo": {
            "code": "src_lambda/echo/src",
            "test": "src_lambda/echo/test/test.json",
            "timeout": 3
        },
        "called_by_batch": {
            "code": "src_lambda/called_by_batch/src",
            "timeout": 3
        },
        "numpy": {
            "code": "src_lambda/numpy/src",
            "test": "src_lambda/numpy/test/test.json",
            "timeout": 3,
            "layer_list": [
                "common"
            ]
        }
    },
    "lambda_layer": {
        "common": "py3-12-common:2"
    },
    "vpc_for_batch": {
        "subnet_id_list": [],
        "security_group_id": ""
    },
    "batch_func": {
        "convert": {
            "queue_state_lambda": "called_by_batch",
            "maxv_cpus": 40,
            "memory": 8192,
            "vcpu": 4
        }
    }
}